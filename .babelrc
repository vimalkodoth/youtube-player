{
  "presets": [
    [
      "@babel/preset-react"
    ],
    "@emotion/babel-preset-css-prop",
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": "10"
        }
      }
    ],
    "@babel/preset-typescript"
  ], //a preset is bunch of plugins for transformation
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "react-hot-loader/babel",
    "@babel/plugin-transform-strict-mode"
  ]
}